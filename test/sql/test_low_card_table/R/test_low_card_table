-- !result
select count(*) from low_card_duplicate where c3 = 'shenzhen' and (c0 = 2838958 or c2 = 'M');
-- result:
0
-- !result
select count(*) from low_card_duplicate where (c1 = 'vRLFJG' or c0 = abs(2924349)) and (c0 = 2838958 or c2 = 'M');
