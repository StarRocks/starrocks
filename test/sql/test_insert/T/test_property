CREATE table tab1 (
      k1 INTEGER,
      k2 VARCHAR(50),
      v1 SMALLINT,
      v2 INTEGER,
      v3 INTEGER,
      v4 varchar(50),
      v5 varchar(50)
)
ENGINE=OLAP
PRIMARY KEY(`k1`,`k2`)
DISTRIBUTED BY HASH(`k1`) BUCKETS 10
PROPERTIES (
    "replication_num" = "1"
);

insert into tab1 values (100, "k2_100", 100, 100, 100, "v4_100", "v5_100");
insert into tab1 values (200, "k2_200", 200, 200, 200, "v4_200", "v5_200");
select * from tab1;

-- partial update
insert into tab1 (k1, k2, v1) properties('partial_update'='true') values (100, "k2_100", 200);
select * from tbl1 where k1 = 100;

-- max filter ratio
insert into tab1 (k1, k2, v1) properties('max_filter_ratio'='0.1') values (400, "k2_100", 100), (500, "k2_100", 500);
insert into tab1 (k1, k2, v1) properties('max_filter_ratio'='0.8') values (400, "k2_100", 100), (500, "k2_100", 500);
select * from tbl1 where k1 = 400;
