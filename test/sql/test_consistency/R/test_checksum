-- name: test_checksum
CREATE table t1 (
      k1 bigint,
      k2 double,
      k3 struct<a bitmap, b bitmap>
)
ENGINE=OLAP
DUPLICATE KEY(`k1`)
DISTRIBUTED BY HASH(`k1`) BUCKETS 1
PROPERTIES (
    "replication_num" = "1"
);
-- result:
-- !result

insert into t1 values (100, 10.1, named_struct("a", bitmap_from_string("1"), "b", bitmap_from_string("1,2")));
-- result:
-- !result

[UC]tablets=show tablets from t1;
shell: curl -u root: "${url}/api/checksum?tablet_id=${tablets[0][0]}&version=2"
-- result:
100
-- !result
