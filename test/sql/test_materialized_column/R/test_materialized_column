  `id` bigint(20) NOT NULL COMMENT "",
  `newcol1` bigint(20) NULL AS id * 10 COMMENT "",
  `newcol2` bigint(20) NULL AS id * 100 COMMENT ""
) ENGINE=OLAP 
UNIQUE KEY(`id`)
DISTRIBUTED BY HASH(`id`) BUCKETS 7 
PROPERTIES (
"replication_num" = "1",
"in_memory" = "false",
"enable_persistent_index" = "false",
"replicated_storage" = "true",
"compression" = "LZ4"
);
-- !result
