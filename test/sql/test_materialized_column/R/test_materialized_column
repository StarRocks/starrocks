  `v2` bigint(20) NOT NULL COMMENT "",
  `v3` bigint(20) NOT NULL COMMENT ""
) ENGINE=OLAP 
DUPLICATE KEY(`v1`)
DISTRIBUTED BY HASH(`v1`) BUCKETS 1 
PROPERTIES (
