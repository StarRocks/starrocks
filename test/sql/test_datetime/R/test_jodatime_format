-- name: test_jodatime_format
select jodatime_format('2023-04-15', 'yyyy-MM-dd');
-- result:
2023-04-15
-- !result
select jodatime_format('2023-04-15 23:45:12', "yyyy-MM-dd'T'HH:mm:ssZZ");
-- result:
2023-04-15T23:45:12+08:00
-- !result
select jodatime_format('2023-04-15 15:30:00', 'yyyy-MM-dd hh:mm:ssa');
-- result:
2023-04-15 03:30:00PM
-- !result
select jodatime_format('2023-12-25', "YYYY-'W'ww-e");
-- result:
2023-W52-1
-- !result
select jodatime_format('2023-08-03', 'MMM dd');
-- result:
Aug 03
-- !result
select jodatime_format('2023-02-29', 'yyyy-MM-dd');
-- result:
None
-- !result
select jodatime_format('2023-04-15', 'yyyy/MM/dd');
-- result:
2023/04/15
-- !result
select jodatime_format('2023-04-15', 'HH:mm:ss');
-- result:
00:00:00
-- !result
select jodatime_format('2023-04-15', 'HH:mm:ss==a--b==cc--aa');
-- result:
00:00:00==AM--b==cc--AM
-- !result
select jodatime_format('2023-01-01 00:00:00.010000', "yyyy-MM-dd HH:mm:ss.SSSSSS");
-- result:
2023-01-01 00:00:00.010000
-- !result
select jodatime_format('2023-01-01 00:00:00.038000', "yyyy-MM-dd HH:mm:ss.SSSSSS");
-- result:
2023-01-01 00:00:00.038000
-- !result
select jodatime_format('2023-01-01 00:00:00.038000', "yyyy-MM-dd HH:mm:ss.SSSSS");
-- result:
2023-01-01 00:00:00.03800
-- !result
select jodatime_format('2023-01-01 00:00:00.038000', "yyyy-MM-dd HH:mm:ss.SSSS");
-- result:
2023-01-01 00:00:00.0380
-- !result
select jodatime_format('2023-01-01 00:00:00.038000', "yyyy-MM-dd HH:mm:ss.SSS");
-- result:
2023-01-01 00:00:00.038
-- !result
select jodatime_format('2023-01-01 00:00:00.038000', "yyyy-MM-dd HH:mm:ss.SS");
-- result:
2023-01-01 00:00:00.03
-- !result
select jodatime_format('2023-01-01 00:00:00.038000', "yyyy-MM-dd HH:mm:ss.S");
-- result:
2023-01-01 00:00:00.0
-- !result
select jodatime_format('2023-06-25 00:00:00', "yyyyMMdd--'01234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345'");
-- result:
20230625--01234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345
-- !result
select jodatime_format('2023-06-25 00:00:00', "yyyyMMdd--'01234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345");
-- result:
20230625--01234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345
-- !result
