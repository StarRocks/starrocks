    ('minute', '2016-03-01 00:00:00.000', '2016-02-27 00:00:00.001'),
    ('second', '2016-03-01 00:00:00.000', '2016-02-27 00:00:00.001'),
    ('millisecond', '2016-03-01 00:00:00.000', '2016-02-27 00:00:00.001'),

    (NULL, '2016-03-01', '2016-02-27'),
    ('day', NULL, '2016-02-27'),
    ('day', '2016-02-27', NULL);
