-- name: test_unixtime_to_datetime
set sql_dialect='StarRocks';
SELECT unixtime_to_datetime(1598306400, 0);
SELECT unixtime_to_datetime(1598306400);
SELECT unixtime_to_datetime(1598306400123, 3);
SELECT unixtime_to_datetime(1598306400123456, 6);
SELECT unixtime_to_datetime(1598306400.123, 3);
SELECT unixtime_to_datetime(1598306400.123456, 6);
SELECT unixtime_to_datetime(0, 0);
SELECT unixtime_to_datetime(253402243199, 0);
SELECT unixtime_to_datetime(1598306400, 10);
SELECT unixtime_to_datetime(1598306400, null);
SELECT unixtime_to_datetime(null, null);
SELECT unixtime_to_datetime(t.ts_val, t.sc_val) AS dyn_scale
FROM (VALUES
        (1598306400123456, 6),
        (1598306400123,    3),
        (1598306400,       0),
        (-1001,            3),
        (NULL,             0),
        (1598306400,       NULL)
     ) AS t(ts_val, sc_val);