-- name: test_jodatime_format

select jodatime_format('2023-04-15', 'yyyy-MM-dd');

select jodatime_format('2023-04-15 23:45:12', "yyyy-MM-dd'T'HH:mm:ssZZ");

select jodatime_format('2023-04-15 15:30:00', 'yyyy-MM-dd hh:mm:ssa');

select jodatime_format('2023-12-25', "YYYY-'W'ww-e");

select jodatime_format('2023-08-03', 'MMM dd');

select jodatime_format('2023-02-29', 'yyyy-MM-dd');

select jodatime_format('2023-04-15', 'yyyy/MM/dd');

select jodatime_format('2023-04-15', 'HH:mm:ss');

select jodatime_format('2023-04-15', 'HH:mm:ss==a--b==cc--aa');

select jodatime_format('2023-01-01 00:00:00.010000', "yyyy-MM-dd HH:mm:ss.SSSSSS");
select jodatime_format('2023-01-01 00:00:00.038000', "yyyy-MM-dd HH:mm:ss.SSSSSS");
select jodatime_format('2023-01-01 00:00:00.038000', "yyyy-MM-dd HH:mm:ss.SSSSS");
select jodatime_format('2023-01-01 00:00:00.038000', "yyyy-MM-dd HH:mm:ss.SSSS");
select jodatime_format('2023-01-01 00:00:00.038000', "yyyy-MM-dd HH:mm:ss.SSS");
select jodatime_format('2023-01-01 00:00:00.038000', "yyyy-MM-dd HH:mm:ss.SS");
select jodatime_format('2023-01-01 00:00:00.038000', "yyyy-MM-dd HH:mm:ss.S");

-- result length is 126
-- 8(yyyyMMdd)+2(--)+116('text')
select jodatime_format('2023-06-25 00:00:00', "yyyyMMdd--'01234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345'");
select jodatime_format('2023-06-25 00:00:00', "yyyyMMdd--'01234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345");
select jodatime_format('2023-06-25 00:00:00', "yyyyMMdd--01234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345");
select jodatime_format('2023-06-25 00:00:00', "'01234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345'--yyyyMMdd");
select jodatime_format('2023-06-25 00:00:00', "01234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345--yyyyMMdd");
select jodatime_format('2023-06-25 00:00:00', "'0123456789012345678901234567890123456789'--yyyyMMdd--'23456789012345678901234567890123456789012345678901234567890123456789012345'");
select jodatime_format('2023-06-25 00:00:00', "0123456789012345678901234567890123456789--yyyyMMdd--23456789012345678901234567890123456789012345678901234567890123456789012345");


-- result length is 127
-- 8(yyyyMMdd)+2(--)+117('text')
select jodatime_format('2023-06-25 00:00:00', "yyyyMMdd--'012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456'");
select jodatime_format('2023-06-25 00:00:00', "yyyyMMdd--'012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456");
select jodatime_format('2023-06-25 00:00:00', "yyyyMMdd--012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456");
select jodatime_format('2023-06-25 00:00:00', "'012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456'--yyyyMMdd");
select jodatime_format('2023-06-25 00:00:00', "012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456--yyyyMMdd");
select jodatime_format('2023-06-25 00:00:00', "'0123456789012345678901234567890123456789'--yyyyMMdd--'234567890123456789012345678901234567890123456789012345678901234567890123456'");
select jodatime_format('2023-06-25 00:00:00', "0123456789012345678901234567890123456789--yyyyMMdd--234567890123456789012345678901234567890123456789012345678901234567890123456");



-- result length is 128
-- 8(yyyyMMdd)+2(--)+118('text')
select jodatime_format('2023-06-25 00:00:00', "yyyyMMdd--'0123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567'");
select jodatime_format('2023-06-25 00:00:00', "yyyyMMdd--'0123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567");
select jodatime_format('2023-06-25 00:00:00', "yyyyMMdd--0123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567");
select jodatime_format('2023-06-25 00:00:00', "'0123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567'--yyyyMMdd");
select jodatime_format('2023-06-25 00:00:00', "0123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567--yyyyMMdd");
select jodatime_format('2023-06-25 00:00:00', "'0123456789012345678901234567890123456789'--yyyyMMdd--'2345678901234567890123456789012345678901234567890123456789012345678901234567'");
select jodatime_format('2023-06-25 00:00:00', "0123456789012345678901234567890123456789--yyyyMMdd--2345678901234567890123456789012345678901234567890123456789012345678901234567");

