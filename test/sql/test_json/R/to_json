-- name: test_to_json
drop database if exists test_to_json;
-- result:
-- !result
create database test_to_json;
-- result:
-- !result
use test_to_json;
-- result:
-- !result
CREATE TABLE `t0` (
  `c0` int(11) NULL,
  `c1` struct<a int(11), b int(11)> NULL
) 
DUPLICATE KEY(`c0`);
-- result:
-- !result
INSERT INTO t0 VALUES (1, NULL);
-- result:
-- !result
INSERT INTO t0 VALUES (2, row(1, 1));
-- result:
-- !result
select to_json(c1) from t0 order by c0;
-- result:
None
{"a": 1, "b": 1}
-- !result
SELECT to_json(row(1, 1));
-- result:
{"col1": 1, "col2": 1}
-- !result
SELECT to_json(NULL);
-- result:
None
-- !result