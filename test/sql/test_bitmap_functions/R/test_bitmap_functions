-- result:
-- !result
CREATE TABLE test_bitmap_table1(
    k1 INT,
    v1 BITMAP BITMAP_UNION
) AGGREGATE KEY(k1)
DISTRIBUTED BY HASH(k1) BUCKETS 3;
-- result:
-- !result
insert into test_bitmap_table1 select 0, NULL;
-- result:
-- !result
