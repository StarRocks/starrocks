-- name: test_cast_json_to_map
CREATE TABLE t (
    c1 int,
    c2 json
) PROPERTIES ("replication_num" = "1");

insert into map_top_n values
(1, map{"key1":1}),
(2, map{"key1":5, "key2":6}),
(3, map{"key1":2, "key2":3, "key4":4}),
(4, map{"key1":12, "key2":13, "key3":14, "key4":15}),
(5, map{"key1":7, "key2":8, "key3":9, "key4":10, "key5":11});