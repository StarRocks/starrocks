-- name: test_approx_top_k_const
SELECT approx_top_k(col1) FROM (VALUES (1)) AS tmp(col1);
-- result:
[{"item":1,"count":1}]
-- !result
SELECT approx_top_k(col1) FROM (VALUES (1),(1),(1),(1),(1),(1)) AS tmp(col1);
-- result:
[{"item":1,"count":6}]
-- !result
SELECT approx_top_k(1);
-- result:
[{"item":1,"count":1}]
-- !result