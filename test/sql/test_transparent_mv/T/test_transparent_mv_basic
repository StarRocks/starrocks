INSERT INTO t2 VALUES (3, "2020-06-18");
ALTER MATERIALIZED VIEW test_mv1 set("transparent_mv_rewrite_mode" = "false");
SELECT dt, num FROM test_mv1 order by 1, 2 limit 3;
ALTER MATERIALIZED VIEW test_mv1 set("transparent_mv_rewrite_mode" = "true");
SELECT dt, num FROM test_mv1 order by 1, 2 limit 3;
ALTER MATERIALIZED VIEW test_mv1 set("transparent_mv_rewrite_mode" = "transparent_or_error");
SELECT dt, num FROM test_mv1 order by 1, 2 limit 3;
ALTER MATERIALIZED VIEW test_mv1 set("transparent_mv_rewrite_mode" = "transparent_or_default");
SELECT dt, num FROM test_mv1 order by 1, 2 limit 3;

drop materialized view default_catalog.db_${uuid0}.test_mv1;

drop table t1 force;
drop table t2 force;