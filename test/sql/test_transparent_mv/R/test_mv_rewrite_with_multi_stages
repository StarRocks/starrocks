  num int,
  dt date
)
PARTITION BY (dt);
-- result:
-- !result
INSERT INTO mv_hive_${uuid0}.mv_hive_db_${uuid0}.t3 VALUES 
  (1,"2020-06-15"),(2,"2020-06-18"),(3,"2020-06-21"),(4,"2020-06-24"), (NULL,"2020-06-24");
-- result:
-- !result
CREATE TABLE mv_hive_${uuid0}.mv_hive_db_${uuid0}.t4 (
  num int,
  dt date
)
PARTITION BY (dt);
