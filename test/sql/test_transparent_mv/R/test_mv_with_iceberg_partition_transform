2025-02-03
2025-03-03
2025-04-01
-- !result
SELECT distinct prcdate FROM mv_iceberg_${uuid0}.sql_test_db.test_iceberg_with_month where prcdate < '2025-01-03' order by prcdate;
-- result:
2025-01-01
2025-01-02
-- !result
SELECT distinct prcdate FROM mv_iceberg_${uuid0}.sql_test_db.test_iceberg_with_month where prcdate > '2025-01-03' order by prcdate;
-- result:
2025-02-03
2025-03-03
2025-04-01
-- !result
select distinct prcdate from test_mv1 order by prcdate;
-- result:
