avg_c5	double	YES	false	None	NONE
cnt_c1	bigint	YES	false	None	NONE
max_c6	int	YES	false	None	NONE
-- !result
INSERT INTO t2_agg VALUES ('2026-01-01', 0, 'new_val', 100, 1000, 'replace_new', 10.5, 50);
