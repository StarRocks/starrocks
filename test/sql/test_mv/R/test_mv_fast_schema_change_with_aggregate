c4	varchar(100)	YES	false	None	
c5	double	YES	false	None	
c6	int	YES	false	None	
-- !result
ALTER MATERIALIZED VIEW test_mv_agg2 ADD COLUMN max_c6 AS MAX(c6);
-- result:
