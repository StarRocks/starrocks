-- type
with w1 as (
    select * from t2 order by k1 limit 10
)
select count(1)
from t1 join [broadcast] w1 using(c_bool_1_null);

 with w1 as (
    select * from t2 order by k1 limit 10
)
select count(1)
from t1 join [broadcast] w1 using(c_bool_2_notnull);

select count(1)
from t1 join [broadcast] t2 using(c_tinyint_1_null);

 
