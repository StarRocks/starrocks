-- name: test_load_channel_profile
CREATE TABLE `t0` (
  `v1` int(11) NOT NULL,
  `v2` int(11) NOT NULL,
  `v3` int(11) NOT NULL
) ENGINE=OLAP
DUPLICATE KEY(`v1`)
DISTRIBUTED BY HASH(`v1`) BUCKETS 10
PROPERTIES (
 "replication_num" = "1"
);
-- result:
-- !result
CREATE MATERIALIZED VIEW `mv1` AS
SELECT `v1`, SUM(`v2`) FROM `t0`
GROUP BY `v1`;
-- result:
-- !result
function: wait_materialized_view_finish()
-- result:
None
-- !result
CREATE MATERIALIZED VIEW `mv2` AS
SELECT `v1`, MAX(`v3`) FROM `t0`
GROUP BY `v1`;
-- result:
-- !result
function: wait_materialized_view_finish()
-- result:
None
-- !result
INSERT INTO `t0` (v1, v2, v3) values
    (1, 1, 1),
    (1, 1, 2),
    (1, 1, 3),
    (1, 2, 4),
    (1, 2, 5),
    (1, 2, 6),
    (2, 3, 7),
    (2, 3, 8),
    (2, 3, 9),
    (2, 4, 10),
    (2, 4, 11),
    (2, 4, 12);
-- result:
-- !result
SET enable_profile=true;
-- result:
-- !result
SET enable_async_profile=false;
