-- name: test_table_function_elimination
CREATE TABLE `t0` (
  `id` int(11) NULL COMMENT "",
  `xs` array<int(11)> NULL COMMENT ""
) ENGINE=OLAP 
DUPLICATE KEY(`id`)
PROPERTIES (
"replication_num" = "1"
);
insert into t0 values
(1,NULL),
