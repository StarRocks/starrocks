select * from t0 x1 where x1.v2 not in (select x2.v2 from t0 x2 where x1.v3 = x2.v2);
-- result:
-- !result
select * from t0 x1 where x1.v4 not in (select x2.v2 from t0 x2 where x1.v2 = x2.v2);
-- result:
