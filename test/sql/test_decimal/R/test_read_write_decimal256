INSERT INTO decimal_test_p50_s45 SELECT 1234567.123456789012345678901234567890123456789012345; -- 7+45=52 > 50, should cause BE exception
SELECT * FROM decimal_test_p50_s45 ORDER BY d1;
-- result:
[REGEX]E*
-- !result
