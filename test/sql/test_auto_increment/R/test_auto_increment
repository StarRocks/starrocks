-- result:
-- !result
insert into t_auto_increment_insert_partial_update (k) values (1),(2),(3),(4);
-- result:
-- !result
insert into t_auto_increment_insert_partial_update (k) values (1),(2),(3),(4);
-- result:
-- !result
insert into t_auto_increment_insert_partial_update (k) values (1),(2),(3),(4);
-- result:
-- !result
insert into t_auto_increment_insert_partial_update (k) values (1),(2),(3),(4);
-- result:
-- !result
insert into t_auto_increment_insert_partial_update (k) values (1),(2),(5);
