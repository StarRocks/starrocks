select * from t0 where (case
   when ship_code >= 90 then 'A'
   when ship_code >= 80 then 'B'
   when ship_code >= 70 then 'C'
   when ship_code >= 60 then 'D'
   else 'E' end)  = 'A' order by 1,2,3,4,5;
