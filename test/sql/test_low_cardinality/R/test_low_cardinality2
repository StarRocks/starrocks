  `v1` bigint(20) NULL COMMENT "",
  `v2` int NULL,
  `v3` varchar(65533) NULL COMMENT "",
  `v4` varchar(65533) NULL COMMENT ""
) ENGINE=OLAP
DUPLICATE KEY(`v1`)
COMMENT "OLAP"
DISTRIBUTED BY HASH(`v1`) BUCKETS 10
PROPERTIES (
"replication_num" = "1",
"in_memory" = "false",
"enable_persistent_index" = "false",
"replicated_storage" = "false",
"compression" = "LZ4"
);
