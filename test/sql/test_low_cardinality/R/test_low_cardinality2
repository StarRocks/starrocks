SELECT MAX(x1), MIN(x2), MAX(y1), MIN(y2) FROM 
(SELECT REVERSE(v3) x1, CONCAT(v4, "1") y1, s2.v1 FROM s2 ) t1
JOIN[SHUFFLE] 
(SELECT lower(v3) x2, upper(v4) y2, s2.v1 FROM s2 ) t2
on t1.v1 = t2.v1
where x2 = "anhui"
GROUP BY x1
HAVING COUNT(y2) > 12280;
-- result:
-- !result
SELECT MAX(x1), MIN(x2), MAX(y1), MIN(y2) FROM
(SELECT REVERSE(v3) x1, CONCAT(v4, "1") y1, s2.v1 FROM s2 ) t1
JOIN[BROADCAST]
(SELECT lower(v3) x2, upper(v4) y2, s2.v1 FROM s2 ) t2
on t1.v1 = t2.v1
where x2 = "anhui"
GROUP BY x1
HAVING COUNT(y2) > 12280;
-- result:
-- !result
SELECT MAX(x1), MIN(x2), MAX(y1), MIN(y2) FROM
(SELECT REVERSE(v3) x1, CONCAT(v4, "1") y1, s2.v1 FROM s2 ) t1
JOIN[SHUFFLE]
(SELECT lower(v3) x2, upper(v4) y2, s2.v1 FROM s2 ) t2
on t1.v1 = t2.v1
where x2 = "anhui"
GROUP BY x1
HAVING MAX(y2) = "JX";
-- result:
nauhciS	anhui	SH1	JX
ixgnauG	anhui	QH1	JX
