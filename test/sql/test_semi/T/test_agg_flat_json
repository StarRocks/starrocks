select JSON_EXISTS(j1, "$.key12"), JSON_EXISTS(j2, "$.key13"), JSON_EXISTS(j2, "$") from ajs1 order by v1 limit 2;
select JSON_LENGTH(j1, "$.key8"), JSON_LENGTH(j2, "$.key9"), JSON_LENGTH(j2, "$.key10") from ajs1 order by v1 limit 2;
select get_json_string(j1, "asdf"), get_json_double(j2, "$13"), get_json_string(j2, "$.key3") from ajs1 order by v1 limit 2;
select JSON_LENGTH(j1, "$.key12"), JSON_LENGTH(j2, "$.key13"), JSON_LENGTH(j2, "$") from ajs1 where v1 = -1 order by v1 limit 2;

-- schema change
ALTER TABLE ajs1 ADD COLUMN j3 JSON REPLACE DEFAULT NULL AFTER j2;
ALTER TABLE ajs1 ADD COLUMN j4 JSON REPLACE DEFAULT NULL AFTER j1;

select get_json_string(j1, "$.key2"), get_json_double(j3, "$.key3"), get_json_string(j3, "$.key4") from ajs1 order by v1 limit 2;
select get_json_string(j1, "$.key2"), get_json_double(j3, "$.key2.key3") from ajs1 order by v1 limit 2;
select JSON_EXISTS(j1, "$.key12"), JSON_EXISTS(j3, "$.key13"), JSON_EXISTS(j3, "$") from ajs1 order by v1 limit 2;
select JSON_EXISTS(j1, "asdf"), JSON_EXISTS(j3, "$13"), JSON_EXISTS(j3, "$.key3") from ajs1 order by v1 limit 2;
select JSON_LENGTH(j1, "$.key2"), JSON_LENGTH(j3, "$.key2.key3") from ajs1 order by v1 limit 2;
select JSON_LENGTH(j1, "asdf"), JSON_LENGTH(j3, "$13"), JSON_LENGTH(j3, "$.key3") from ajs1 order by v1 limit 2;
select JSON_LENGTH(j1, "$.key2"), JSON_LENGTH(j3, "$.key2.key3") from ajs1 where v2 = 2 order by v1 limit 2;
select JSON_LENGTH(j1, "$.key12"), JSON_LENGTH(j3, "$.key13"), JSON_LENGTH(j3, "$") from ajs1 where v1 = -1 order by v1 limit 2;
select JSON_LENGTH(j1, "$.key12"), JSON_LENGTH(j3, "$.key13"), JSON_LENGTH(j3, "$") from ajs1 where v1 = -1 order by v1 limit 2;
select JSON_LENGTH(j1, "$.key12"), JSON_LENGTH(j3, "$.key13"), JSON_LENGTH(j3, "$") from ajs1 where v1 = -1 order by v1 limit 2;

select get_json_string(j4, "$.key2"), get_json_double(j3, "$.key3"), get_json_string(j3, "$.key4") from ajs1 order by v1 limit 2;
select get_json_string(j4, "$.key2"), get_json_double(j3, "$.key2.key3") from ajs1 order by v1 limit 2;
select JSON_EXISTS(j4, "$.key12"), JSON_EXISTS(j3, "$.key13"), JSON_EXISTS(j3, "$") from ajs1 order by v1 limit 2;
select JSON_EXISTS(j4, "asdf"), JSON_EXISTS(j3, "$13"), JSON_EXISTS(j2, "$.key3") from ajs1 order by v1 limit 2;
select JSON_LENGTH(j4, "$.key2"), JSON_LENGTH(j2, "$.key2.key3") from ajs1 order by v1 limit 2;
select JSON_LENGTH(j4, "asdf"), JSON_LENGTH(j2, "$13"), JSON_LENGTH(j2, "$.key3") from ajs1 order by v1 limit 2;
select JSON_LENGTH(j4, "$.key2"), JSON_LENGTH(j2, "$.key2.key3") from ajs1 where v2 = 2 order by v1 limit 2;
select JSON_LENGTH(j4, "$.key12"), JSON_LENGTH(j2, "$.key13"), JSON_LENGTH(j3, "$") from ajs1 where v1 = -1 order by v1 limit 2;
select JSON_LENGTH(j4, "$.key12"), JSON_LENGTH(j3, "$.key13"), JSON_LENGTH(j3, "$") from ajs1 where v1 = -1 order by v1 limit 2;
select JSON_LENGTH(j4, "$.key12"), JSON_LENGTH(j3, "$.key13"), JSON_LENGTH(j2, "$") from ajs1 where v1 = -1 order by v1 limit 2;


ALTER TABLE ajs1 DROP COLUMN j1;
ALTER TABLE ajs1 DROP COLUMN j4;

select JSON_EXISTS(j2, "$.key12"), JSON_EXISTS(j2, "$.key13"), JSON_EXISTS(j2, "$") from ajs1 order by v1 limit 2;
select JSON_LENGTH(j2, "$.key8"), JSON_LENGTH(j2, "$.key9"), JSON_LENGTH(j2, "$.key10") from ajs1 order by v1 limit 2;
