insert into sc3 values (4, [9,2,3],NULL, NULL, NULL, NULL);
insert into sc3 values (4, [9,2,3],NULL, NULL, NULL, NULL);
insert into sc3 values (4, [9,2,3],NULL, NULL, NULL, NULL);

insert into t0 values (1, 1, '1', '1');
insert into t0 values (2, 2, '2', '2');
insert into t0 values (3, 3, '3', '3');
