-- name: test_warehouse_queries
CREATE DATABASE db_${uuid0};
-- result:
-- !result
USE db_${uuid0};
-- result:
-- !result
desc information_schema.warehouse_queries;
-- result:
WAREHOUSE_ID	varchar(512)	YES	false	None	
