-- name: test_random
select random(1);
-- result:
0.303263684252395
-- !result
select random(10);
-- result:
0.04054825027038398
-- !result
select random(1) rnd from table(generate_series(1, 10)) order by rnd;
-- result:
0.29807723660665303
0.303263684252395
0.3299691983558783
0.42813594232989677
0.517568710535781
0.640347459629553
0.6418163354732807
0.7591263240225643
0.833037205570971
0.881035884971337
-- !result
select uniform_distribution(1, 100, 1);
-- result:
31
-- !result
select uniform_distribution(1, 100, 10);
-- result:
5
-- !result
select uniform_distribution(1, 100, 10) rnd from table(generate_series(1, 10)) order by rnd;
-- result:
5
14
22
23
25
28
29
71
76
87
-- !result
select normal_distribution(1, 100, 1);
-- result:
-63.01020571085887
-- !result
select normal_distribution(1, 100, 10);
-- result:
72.50572802138228
-- !result
select normal_distribution(1, 100, 10) rnd from table(generate_series(1, 10)) order by rnd;
