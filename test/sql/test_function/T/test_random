-- name: test_random

select random(1);
select random(10);
select random(1) rnd from table(generate_series(1, 10)) order by rnd;

select uniform_distribution(1, 100, 1);
select uniform_distribution(1, 100, 10);
select uniform_distribution(1, 100, 10) rnd from table(generate_series(1, 10)) order by rnd;


select normal_distribution(1, 100, 1);
select normal_distribution(1, 100, 10);
select normal_distribution(1, 100, 10) rnd from table(generate_series(1, 10)) order by rnd;

select poisson_distribution(100, 1);
select poisson_distribution(100, 10);
select poisson_distribution(100, 10) rnd from table(generate_series(1, 10)) order by rnd;