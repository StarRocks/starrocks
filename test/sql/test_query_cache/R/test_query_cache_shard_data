SELECT /*+SET_VAR(enable_query_cache=true, pipeline_dop=1) */
    sum(v1) AS __c_0
FROM
    t0_pk
WHERE
    ts between '2022-01-03 00:00:00'
    and '2022-01-03 23:59:59'
GROUP BY
    date_trunc('hour', ts),
    k0
ORDER BY
    date_trunc('hour', ts);
