CREATE TABLE `primary_key_with_null` ( 
    `k1`  date, 
    `k2`  datetime, 
    `k3`  varchar(20), 
    `k4`  varchar(20), 
    `k5`  boolean, 
    `k6`  tinyint, 
    `k7`  smallint, 
    `k8`  int, 
    `K9`  bigint, 
    `K10` largeint, 
    `K11` float, 
    `K12` double, 
    `K13` decimal(27,9) 
) PRIMARY KEY(`k1`, `k2`, `k3`) 
DISTRIBUTED BY HASH(`k1`, `k2`, `k3`) BUCKETS 3 
PROPERTIES ( "replication_num" = "1");
