    (
      (
        c_int_1_seq = 1280001
        or
        c_date_1_seq < '1900-01-01' -- always false
      )
      and
      c_datetime_1_seq < '2040-01-01' -- always true
    )
    or 
    (
      c_str_1_seq = 'abc1-2'
      and c_str_3_low1 = 'a1'
    )
  )
  and 
  (
    c_str_1_seq = 'abc1-1'
    or
    c_date_1_seq = '2020-01-01'
  );
