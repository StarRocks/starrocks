  `k` int(11) NULL COMMENT "",
  `v` int(11) NULL COMMENT ""
) ENGINE=OLAP 
DUPLICATE KEY(`k`, `v`)
PARTITION BY RANGE(`k`)
(PARTITION p1 VALUES [("-2147483648"), ("0")),
