insert into t1(c_key, c_datetime) values
    (1, "2024-01-01 00:00:00"),
    (2, "2024-01-01 00:00:00"),
    (3, "2024-01-01 00:00:00"),
    (4, "2024-01-02 00:00:00"),
    (5, "2024-01-02 00:00:00"),
    (6, "2024-01-03 00:00:00");
