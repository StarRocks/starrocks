select s.s_1, t.i_1 from array_test s join array_test t on s.s_1 = t.i_1 order by 1, 2;
select s.s_1, t.i_1 from array_test s join array_test t on s.s_1 <=> t.i_1 order by 1, 2;
select s.s_1, t.i_1 from array_test s join array_test t on s.s_1 > t.i_1 order by 1, 2;
select s.s_1, t.i_1 from array_test s join array_test t on s.s_1 < t.i_1 order by 1, 2;

