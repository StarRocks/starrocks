-- !result
-- name: test11
SELECT sum(y)=2*(5000*5001/2) FROM TABLE(generate_series(1, 2)) t(x), LATERAL generate_series(1, 5000) t2(y);
-- result:
1
-- !result
-- name: test10
SELECT * FROM TABLE(generate_series(1, 5)) t(x), LATERAL generate_series(x, x+1);
-- result:
