-- name: test_partial_update_many_columns
show backends;
create database test_partial_update_many_columns;
-- result:
-- !result
use test_partial_update_many_columns;
-- result:
-- !result
CREATE table tab1 (
      k1 INTEGER,
      k2 VARCHAR(50),
      v1 INTEGER,
      v2 INTEGER,
      v3 INTEGER,
      v4 varchar(50),
      v5 varchar(50),
      v6 INTEGER,
      v7 INTEGER,
      v8 INTEGER,
      v9 varchar(50),
      v10 varchar(50),
      v11 INTEGER,
      v12 INTEGER,
      v13 INTEGER,
      v14 varchar(50),
      v15 varchar(50)
)
ENGINE=OLAP
PRIMARY KEY(`k1`,`k2`)
DISTRIBUTED BY HASH(`k1`) BUCKETS 10
PROPERTIES (
    "replication_num" = "1"
);
-- result:
-- !result
set partial_update_mode = "column";
