-- name: test_column_partial_update_with_condition_update @cloud
create database test_column_partial_update_with_condition_update;
-- result:
-- !result
use test_column_partial_update_with_condition_update;
-- result:
-- !result
CREATE TABLE site_access (
  site_id int(11) not NULL DEFAULT "10" COMMENT "",
  event_day date NULL COMMENT "",
  city_code varchar(100) NULL COMMENT "",
  user_name varchar(32) NULL DEFAULT "" COMMENT "",
  pv bigint(20) NULL DEFAULT "0" COMMENT ""
) ENGINE=OLAP 
PRIMARY KEY(site_id)
DISTRIBUTED BY HASH(site_id)
PROPERTIES (
"compression" = "LZ4",
"fast_schema_evolution" = "true",
"replication_num" = "1"
);
-- result:
-- !result
insert into site_access values
(22,'2024-07-22','bsgs','zs',22),
(23,'2024-07-23','bsgs','zs',23),
(24,'2024-07-24','bsgs','zs',24),
(25,'2024-07-25','bsgs','zs',25),
(26,'2024-07-26','bsgs','zs',26),
(27,'2024-07-27','bsgs','zs',27),
