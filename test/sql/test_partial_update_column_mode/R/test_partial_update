300	300	300	300
200	200	200	200
-- !result
update tab1 set v1 = (select sum(tab2.v1) from tab2), v2 = (select sum(tab2.v2) from tab2), v3 = 1000;
-- result:
E: (1064, 'Getting analyzing error. Detail message: must specify where clause to prevent full table update.')
-- !result
select * from tab1;
-- result:
