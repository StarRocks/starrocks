300	k3_300	300	300	300	v4_300	v5_300
200	k2_200	200	200	200	v4_200	v5_200
-- !result
update tab1 set v1 = (select sum(tab2.v1) from tab2), v2 = (select sum(tab2.v2) from tab2) where k1 = 100;
-- result:
-- !result
select * from tab1;
-- result:
