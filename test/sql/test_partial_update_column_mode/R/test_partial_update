-- !result
insert into tab2 values (100, 100, 100, 100);
-- result:
-- !result
insert into tab2 values (200, 200, 200, 200);
-- result:
-- !result
insert into tab2 values (300, 300, 300, 300);
-- result:
-- !result
select * from tab2;
-- result:
