select t3.c0, t3.c1, t.c0, t.c1, t.cnt from t3 left join (select count(*) as cnt, t2.c0, t2.c1, max(t2.c2) from t1 left join t2 on t1.c0 =t2.c0 and t1.c1=t2.c1 group by t2.c0, t2.c1) t on t3.c0<=>t.c0 and t3.c1= t.c1;
