select t1.c0, t2.c0, t3.c0, t1.c1, t2.c1, t3.c1 from t1 left join t2 on t1.c0<=>t2.c0 and t1.c1=t2.c1 left join t3 on t2.c0=t3.c0 and t2.c1=t3.c1;
