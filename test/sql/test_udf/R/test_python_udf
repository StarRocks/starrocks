CREATE FUNCTION get_type(struct<a int, b string>) RETURNS 
string 
properties(
    "symbol" = "echo", 
    "type" = "Python",     
    "file" = "inline",
    "input" = "scalar"
)
AS  
$$ 
def echo(x):
    return str(type(x))  
$$;
-- result:
-- !result
CREATE FUNCTION get_type(map<int,string>) RETURNS 
string 
properties(
    "symbol" = "echo", 
    "type" = "Python",     
    "file" = "inline",
    "input" = "scalar"
)
AS  
$$ 
def echo(x):
    return str(type(x))  
$$;
-- result:
-- !result
select get_type(1);
-- result:
<class 'int'>
-- !result
select get_type(1.1);
-- result:
<class 'float'>
-- !result
select get_type('1');
-- result:
<class 'str'>
-- !result
select get_type([]);
-- result:
<class 'list'>
-- !result
select get_type([1,2,3]);
-- result:
<class 'list'>
-- !result
select get_type(named_struct('a', 1,'b','value'));
-- result:
<class 'dict'>
-- !result
select get_type(map(1,2));
-- result:
<class 'list'>
-- !result
CREATE FUNCTION add_one(int) RETURNS 
int 
properties(
    "symbol" = "add", 
    "type" = "Python",     
    "file" = "inline",
    "input" = "scalar"
)
AS  
$$ 
def add(x):
    return x + 1  
$$;
-- result:
-- !result
CREATE FUNCTION add_one(bigint) RETURNS 
bigint 
properties(
    "symbol" = "add", 
    "type" = "Python",     
    "file" = "inline",
    "input" = "scalar"
)
AS  
$$ 
def add(x):
    return x + 1  
$$;
-- result:
-- !result
select add_one(1);
-- result:
2
-- !result
select add_one(2200000000);
-- result:
2200000001
-- !result
