CREATE FUNCTION get_type(struct<a int, b string>) RETURNS 
struct<a int, b string> 
properties(
    "symbol" = "echo", 
    "type" = "Python",     
    "file" = "inline",
    "input" = "arrow"
)
AS  
$$ 
def echo(x):
    return x 
$$;
-- result:
E: (1064, 'function already exists')
-- !result
CREATE FUNCTION vectorized_add_one(int) RETURNS 
int 
properties(
    "symbol" = "add", 
    "type" = "Python",     
    "file" = "inline",
    "input" = "arrow"
)
AS  
$$ 
def add(x):
    return x + 1  
$$;
-- result:
-- !result
CREATE FUNCTION vectorized_add_one(bigint) RETURNS 
bigint 
properties(
    "symbol" = "add", 
    "type" = "Python",     
    "file" = "inline",
    "input" = "arrow"
)
AS  
$$ 
import pyarrow.compute as pc

def add(x):
    return pc.add(x, 1)
$$;
-- result:
-- !result
select add_one(1);
-- result:
2
-- !result
select add_one(2200000000);
-- result:
2200000001
-- !result