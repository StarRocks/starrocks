-- name: test_python_udf
CREATE FUNCTION get_type(boolean) RETURNS 
string 
properties(
    "symbol" = "echo", 
    "type" = "Python",     
    "file" = "inline",
    "input" = "scalar"
)
AS  
$$ 
def echo(x):
    return type(x)  
$$;
-- result:
-- !result
