5	b	2	bb
6	None	None	None
-- !result
select * from target1 order by k1;
-- result:
1	a	1	aa
1	b	2	bb
3	None	None	None
4	a	1	aa
5	b	2	bb
6	None	None	None
-- !result
select * from target2 order by k1;
-- result:
4	1	1
5	1	2
6	0	None
-- !result
drop table if exists test2;
