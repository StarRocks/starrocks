  `k4` varchar(64) NULL
) ENGINE=OLAP 
DUPLICATE KEY(`k1`)
DISTRIBUTED BY HASH(`k1`) BUCKETS 1
PROPERTIES (
"replication_num" = "1"
);
-- result:
-- !result
CREATE MATERIALIZED VIEW test_mv3
to target2
as
select k1, count(k2) as count_k2, sum(k3) as sum_k3 from test2 group by k1;
-- result:
-- !result
insert into test2 values (1, 'a', 1, 'aa'), (1, 'b', 2, 'bb'), (3, NULL, NULL, NULL);
