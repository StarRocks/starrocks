-- result:
-- !result
CREATE MATERIALIZED VIEW test_mv1 
PARTITION BY date_trunc('day', dt)
REFRESH DEFERRED MANUAL AS 
  SELECT dt, sum(num) FROM t3 GROUP BY dt;
-- result:
-- !result
REFRESH MATERIALIZED VIEW test_mv1 WITH SYNC MODE;
