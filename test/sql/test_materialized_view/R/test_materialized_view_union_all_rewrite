-- name: test_materialized_view_union_all_rewrite
CREATE TABLE `mt1` (
 k1 INT,
 k2 string,
 v1 INT,
 v2 INT
) ENGINE=OLAP
PARTITION BY RANGE(`k1`)
(
  PARTITION `p1` VALUES LESS THAN ('3'),
  PARTITION `p2` VALUES LESS THAN ('6'),
  PARTITION `p3` VALUES LESS THAN ('9')
)
DISTRIBUTED BY HASH(`k1`) BUCKETS 3
PROPERTIES (
"replication_num" = "1"
);
-- result:
-- !result
INSERT INTO mt1 values (1,'a',1,1), (2,'aa',1,2),  (3,'a',1,3), (4,'aa',1,4), (5,'aa',1,5), (6,'aa',1,6);
-- result:
-- !result
CREATE MATERIALIZED VIEW mv0 
PARTITION BY (k1) 
DISTRIBUTED BY HASH(k1) 
REFRESH DEFERRED MANUAL 
AS SELECT k1,k2, v1,v2 from mt1 where v2 != 2;
-- result:
-- !result
REFRESH MATERIALIZED VIEW mv0 PARTITION START ('1') END ('3') with sync mode;
set materialized_view_union_rewrite_mode = 0;
-- result:
-- !result
function: check_hit_materialized_view("select * from mt1 where k1 < 3 and v2 != 2", "mv0")
-- result:
None
-- !result
function: check_hit_materialized_view("select * from mt1 where k1 = 1 and v2 >= 4 ", "mv0")
-- result:
None
-- !result
function: check_hit_materialized_view("SELECT k1,k2, v1,v2 from mt1", "mv0", "UNION")
-- result:
None
-- !result
function: check_hit_materialized_view("SELECT distinct k1,k2, v1,v2 from mt1", "mv0", "UNION")
-- result:
None
-- !result
function: check_no_hit_materialized_view("SELECT k1,k2, v1,v2 from mt1 WHERE k1 <6 and v2 != 3", "mv0", "UNION")
-- result:
None
-- !result
function: check_no_hit_materialized_view("SELECT k1,k2, v1,v2 from mt1 where k1<6 and k2 = 'a' and v2 != 4", "mv0")
-- result:
None
-- !result
function: check_no_hit_materialized_view("SELECT k1,k2, v1,v2 from mt1 where k1>0 and k2 = 'a' and v2 >= 4", "mv0")
-- result:
None
-- !result
function: check_no_hit_materialized_view("SELECT k1,k2, v1,v2 from mt1 where v2 != 4", "mv0")
-- result:
None
-- !result
function: check_no_hit_materialized_view("SELECT k1,k2, v1,v2 from mt1 where v2 = 2", "mv0")
-- result:
None
-- !result
function: check_no_hit_materialized_view("SELECT k1,k2, v1,v2 from mt1 where v2 <= 2", "mv0")
-- result:
None
-- !result
select * from mt1 where k1 < 3 and v2 != 2 order by 1;
-- result:
1	a	1	1
-- !result
select * from mt1 where k1 = 1 and v2 >= 4  order by 1;
-- result:
-- !result
select * from mt1 where k1<6 and k2  = 'a' and v2 != 4 order by 1;
-- result:
1	a	1	1
3	a	1	3
-- !result
select * from mt1 where k1>0 and k2  = 'a' and v2 >= 4  order by 1;
-- result:
-- !result
SELECT k1,k2, v1,v2 from mt1 where v2 != 4 order by 1;
-- result:
1	a	1	1
2	aa	1	2
3	a	1	3
5	aa	1	5
6	aa	1	6
-- !result
SELECT k1,k2, v1,v2 from mt1 order by 1;
-- result:
1	a	1	1
2	aa	1	2
3	a	1	3
4	aa	1	4
5	aa	1	5
6	aa	1	6
-- !result
SELECT DISTINCT k1,k2, v1,v2 from mt1 order by 1;
-- result:
1	a	1	1
2	aa	1	2
3	a	1	3
4	aa	1	4
5	aa	1	5
6	aa	1	6
-- !result
SELECT k1,k2, v1,v2 from mt1 where v2 = 2 order by 1;
-- result:
2	aa	1	2
-- !result
SELECT k1,k2, v1,v2 from mt1 where v2 <= 2 order by 1;
-- result:
1	a	1	1
2	aa	1	2
-- !result
SELECT k1,k2, v1,v2 from mt1 WHERE k1 <6 and v2 >= 4  order by 1;
-- result:
4	aa	1	4
5	aa	1	5
-- !result
set materialized_view_union_rewrite_mode = 1;
