PARTITION BY str2date(dt, '%Y-%m-%d')
REFRESH DEFERRED MANUAL AS 
  SELECT dt,sum(num) FROM mv_hive_${uuid0}.mv_hive_db_${uuid0}.t2 GROUP BY dt;
