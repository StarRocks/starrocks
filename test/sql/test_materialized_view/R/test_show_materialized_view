AS select user_id, bitmap_union(to_bitmap(tag_id)) from user_tags group by user_id;
-- !result
refresh materialized view user_tags_mv1 with sync mode;
function: wait_show_materialized_view_finish('user_tags_mv1')
-- result:
None
-- !result
select 
    TABLE_NAME,
    LAST_REFRESH_STATE,
    LAST_REFRESH_ERROR_CODE,
    IS_ACTIVE,
    INACTIVE_REASON
from information_schema.materialized_views where table_name = 'user_tags_mv1' and TABLE_SCHEMA = 'test_show_materialized_view';
-- result:
user_tags_mv1	SKIPPED	0	true	None
-- !result
set @last_refresh_time = (
    select max(last_refresh_start_time)
    from information_schema.materialized_views where table_name = 'user_tags_mv1' and TABLE_SCHEMA = 'test_show_materialized_view'
);
-- result:
-- !result
refresh materialized view user_tags_mv1 force with sync mode;
function: wait_show_materialized_view_finish('user_tags_mv1')
-- result:
None
-- !result
select 
    TABLE_NAME,
    LAST_REFRESH_STATE,
    LAST_REFRESH_ERROR_CODE,
    IS_ACTIVE,
    INACTIVE_REASON
from information_schema.materialized_views where table_name = 'user_tags_mv1' and TABLE_SCHEMA = 'test_show_materialized_view';
-- result:
user_tags_mv1	SUCCESS	0	true	None
-- !result
set @this_refresh_time = (
    select max(last_refresh_start_time)
    from information_schema.materialized_views where table_name = 'user_tags_mv1' and TABLE_SCHEMA = 'test_show_materialized_view'
);
-- result:
-- !result
select if(@last_refresh_time != @this_refresh_time, 
    'refreshed', concat('no refresh after ', @last_refresh_time));
-- result:
refreshed
-- !result
select TABLE_NAME, LAST_REFRESH_ERROR_CODE, IS_ACTIVE, INACTIVE_REASON from information_schema.materialized_views where TABLE_SCHEMA = 'test_show_materialized_view' and table_name = 'user_tags_mv1' ORDER BY LAST_REFRESH_START_TIME DESC;
-- result:
user_tags_mv1	0	true	None
-- !result
