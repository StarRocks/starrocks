-- name: test_spm_manual_create @sequential

create table t1 (
    k1 int,
    k2 int,
    k3 string
)
duplicate key(k1)
distributed by hash(k1) buckets 1
properties("replication_num" = "1");

insert into t1 
select s1, s1 % 1000, repeat('a', 128) FROM TABLE(generate_series(1, 100)) s(s1);

create table t2 like t1;
insert into t2 select * from t1;

create baseline using select * from t1 where k1 = 1 and k2 = 2;

show baseline on select * from t1 where k1 = 1 and k2 = 2;

create baseline on select * from t1 where k1 = 1 and k2 = 2 using select * from t1 where k1 = 3 and k2 = 4;

show baseline on select * from t1 where k1 = 1 and k2 = 2;

create baseline on select * from t1 where k1 = _spm_const_var(100, 1) and k2 = _spm_const_var(101, 2) using select * from t1 where k1 = _spm_const_var(100, 3) and k2 = _spm_const_var(101, 4);

show baseline on select * from t1 where k1 = 1 and k2 = 2;

create baseline using select * from t1 where k1 in _spm_const_list(200, 1, 2, 3);

show baseline on select * from t1 where k1 in (1, 2, 3);

create baseline using select * from t1 where k1 = _spm_const_range(300, 1, 10);

show baseline on select * from t1 where k1 >= 1 and k1 <= 10;

create baseline using select * from t1 where k1 = _spm_const_enum(400, 1, 5, 10);

show baseline on select * from t1 where k1 in (1, 5, 10);