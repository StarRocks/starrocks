-- name: test_spm_plan_rewrite_enable_disable @sequential

create table t1 (
    k1 int,
    k2 int,
    k3 string
)
duplicate key(k1)
distributed by hash(k1) buckets 1
properties("replication_num" = "1");

insert into t1 
select s1, s1 % 1000, repeat('a', 128) FROM TABLE(generate_series(1, 100)) s(s1);

create baseline on select * from t1 where k1 = 1 using select /*+ SET_VAR(parallel_fragment_exec_instance_num=2) */ * from t1 where k1 = 1;

explain select * from t1 where k1 = 1;

select * from t1 where k1 = 1;

drop baseline on select * from t1 where k1 = 1;

explain select * from t1 where k1 = 1;

select * from t1 where k1 = 1;