-- name: test_expr_partition
create database eee;
-- result:
-- !result
use eee;
-- result:
-- !result
create table t(k date, v int) PARTITION BY DATE_TRUNC('DAY', `k`)
PROPERTIES (
"replication_num" = "1",
"bucket_size" = "1"
);
