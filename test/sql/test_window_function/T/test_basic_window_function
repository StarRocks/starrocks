-- ========================================================= LEAD =========================================================

-- LEAD(HAS_NULL) without partition by
SELECT SUM(wv), AVG(wv), MIN(wv), MAX(wv) FROM (SELECT LEAD(v2) OVER(ORDER BY v5) AS wv FROM t1) a;
SELECT SUM(wv), AVG(wv), MIN(wv), MAX(wv) FROM (SELECT LEAD(v2, 3) OVER(ORDER BY v5) AS wv FROM t1) a;
SELECT SUM(wv), AVG(wv), MIN(wv), MAX(wv) FROM (SELECT LEAD(v2, 5, -1) OVER(ORDER BY v5) AS wv FROM t1) a;
SELECT SUM(wv), AVG(wv), MIN(wv), MAX(wv) FROM (SELECT LEAD(v2 IGNORE NULLS) OVER(ORDER BY v5) AS wv FROM t1) a;
SELECT SUM(wv), AVG(wv), MIN(wv), MAX(wv) FROM (SELECT LEAD(v2 IGNORE NULLS, 3) OVER(ORDER BY v5) AS wv FROM t1) a;
SELECT SUM(wv), AVG(wv), MIN(wv), MAX(wv) FROM (SELECT LEAD(v2 IGNORE NULLS, 5, -1) OVER(ORDER BY v5) AS wv FROM t1) a;
-- LEAD(HAS_NULL) with partition by
