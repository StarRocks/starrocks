SELECT SUM(wv), AVG(wv), MIN(wv), MAX(wv) FROM (SELECT LEAD(v2 IGNORE NULLS, 5, -1) OVER(PARTITION BY v1 ORDER BY v5) AS wv FROM t1) a;
-- LEAD(NOT_NULL) without partition by
