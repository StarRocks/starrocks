    SUM(wv1), AVG(wv1), MIN(wv1), MAX(wv1),
    SUM(wv2), AVG(wv2), MIN(wv2), MAX(wv2)
FROM (
    SELECT
        LAG(v3, 3) OVER(ORDER BY v5) AS wv1,
        SUM(v3) OVER(ORDER BY v5 ROWS BETWEEN UNBOUNDED PRECEDING AND 3 PRECEDING) AS wv2
    FROM t1
) a;
-- result:
