CREATE EXTERNAL TABLE dict_two_page
(
    seq bigint,
    f00 string,
    f01 string,
    id string,
    f03 string,
    f04 string
) 
ENGINE=file
PROPERTIES 
(
    "path" = "oss://${oss_bucket}/test_query_external_file/${uuid0}/", 
    "format" = "parquet"
);
