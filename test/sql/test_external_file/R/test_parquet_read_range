    c0 int,
    c1 int,
    c2 string,
    c3 array<int>
)
ENGINE=file
PROPERTIES (
    "path" = "oss://${oss_bucket}/test_external_file/test_parquet_read_range/${uuid0}/read_range_test.parquet",
    "format" = "parquet"
);
