-- name: testExternalParquetDictNullPredicate

shell: ossutil64 mkdir oss://starrocks-env-s3-unit-test/test_external_file/dict_with_null_value
shell: ossutil64 cp --force ../be/test/exec/test_data/parquet_scanner/dict_with_null_value.parquet oss://starrocks-env-s3-unit-test/test_external_file/dict_with_null_value/dict_with_null_value.parquet

-- MySQL [tpch_100g_zhangyan_parquet_lz4]> select c_mktsegment from customer_with_nulls3;
-- +--------------+
