-- | AUTOMOBILE   |
-- | AUTOMOBILE   |
-- | AUTOMOBILE   |
-- | BUILDING     |
-- | FURNITURE    |
-- | NULL         |
-- | BUILDING     |
-- | NULL         |
-- | BUILDING     |
-- | FURNITURE    |
-- | NULL         |
-- | FURNITURE    |
-- | AUTOMOBILE   |
-- | BUILDING     |
-- | NULL         |
-- | FURNITURE    |
-- | MACHINERY    |
-- | MACHINERY    |
-- | NULL         |
-- | MACHINERY    |
-- | FURNITURE    |
-- | AUTOMOBILE   |
-- | BUILDING     |
-- | FURNITURE    |
-- | FURNITURE    |
-- | BUILDING     |
-- +--------------+

CREATE EXTERNAL TABLE tpch_customer_null
(
    c_custkey int,
    c_name string,
    c_address string,
    c_nationkey int,
    c_phone string,
    c_acctbal decimal(15,2),
    c_mktsegment string,
    c_comment string
) 
ENGINE=file
PROPERTIES 
(
    "path" = "oss://starrocks-env-s3-unit-test/test_external_file/dict_with_null_value/", 
    "format" = "parquet"
);

select count(*) from tpch_customer_null;

-- BUILDING = 7, NULL = 6
select count(*) from tpch_customer_null where (c_mktsegment = 'BUILDING' OR c_mktsegment is NULL);

-- BUILDING = 7, NULL = 6
select count(*) from tpch_customer_null where coalesce(c_mktsegment, 'BUILDING') = 'BUILDING';
