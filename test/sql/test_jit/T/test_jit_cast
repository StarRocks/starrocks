-- name: test_jit_cast
DROP TABLE IF EXISTS `jit_cast`;
CREATE TABLE jit_cast (
	k1 BIGINT, 
	k2 BIGINT, 
	v1 BIGINT REPLACE, 
	v2 TINYINT REPLACE, 
	v3 SMALLINT REPLACE, 
	v4 INT REPLACE,
	v5 BIGINT REPLACE,
	v6 LARGEINT REPLACE,
	v7 DECIMAL REPLACE,
	v8 DOUBLE REPLACE,
	v9 FLOAT REPLACE,
	v10 BOOLEAN REPLACE
) 
ENGINE = olap 
AGGREGATE KEY(k1, k2) 
DISTRIBUTED BY HASH(k1) BUCKETS 10 
PROPERTIES ( 
	"replication_num" = "1"
);

