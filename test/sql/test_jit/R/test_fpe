-- name: test_jit_fpe
DROP TABLE IF EXISTS `jit_fpe`;
-- result:
-- !result
CREATE TABLE jit_fpe (
	k1 BIGINT, 
	v1 INT REPLACE, 
	v2 BIGINT REPLACE, 
	v3 INT REPLACE, 
	v4 BIGINT REPLACE,
	v5 DOUBLE REPLACE
) 
ENGINE = olap 
AGGREGATE KEY(k1) 
DISTRIBUTED BY HASH(k1) BUCKETS 10 
PROPERTIES ( "replication_num" = "1" ) 
;
