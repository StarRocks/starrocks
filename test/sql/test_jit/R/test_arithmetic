-- name: test_arithmetic
DROP TABLE IF EXISTS `jit_basic`;
-- result:
-- !result
CREATE TABLE IF NOT EXISTS `jit_basic` (
	k1 BIGINT, 
	k2 BIGINT, 
	v1 BIGINT REPLACE, 
	v2 BIGINT REPLACE, 
	v3 BIGINT REPLACE,
	v4 BIGINT REPLACE, 
	v5 BIGINT REPLACE, 
	v6 BIGINT REPLACE,
	v7 BIGINT REPLACE,
	v8 BIGINT REPLACE,
	v9 BIGINT REPLACE
) 
ENGINE = olap 
AGGREGATE KEY(k1, k2) 
DISTRIBUTED BY HASH(k1) BUCKETS 10 
PROPERTIES( 
	"replication_num" = "1" 
);
