-- name: test_memory_metrics_txn_count
CREATE TABLE `t1` (
    a int
) ENGINE=OLAP
DUPLICATE KEY(`a`)
DISTRIBUTED BY HASH(`a`) BUCKETS 10
PROPERTIES (
"replication_num" = "1"
);
insert into t1 values (1);
shell: curl '${url}/metrics' | grep starrocks_fe_memory | grep \"txn_count\" | awk '{if ($NF >= 1) print "true"; else print "false";}'

