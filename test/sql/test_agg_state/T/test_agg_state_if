CREATE TABLE t0 (
    k1 INT,
    v1 INT,
    v2 INT)
DUPLICATE KEY(k1)
DISTRIBUTED BY HASH(k1)
PROPERTIES(
    "replication_num" = "1"
);
insert into t0 SELECT generate_series % 50, if(generate_series % 2, generate_series, NULL), if(generate_series % 2, 0,1)  FROM TABLE(generate_series(1,  40960));

select sum_if(v1, v2) from t0 group by k1;
select sum_if(v1, v2) from t0 group by k1 limit 2;

set STREAMING_PREAGGREGATION_MODE=force_streaming;
select sum_if(v1, v2) from t0 group by k1;
select sum_if(v1, v2) from t0 group by k1 limit 2;
set STREAMING_PREAGGREGATION_MODE=auto;

CREATE TABLE `t2` (
  `k1` varchar(255) NOT NULL COMMENT "",
  `k2` varchar(255) NOT NULL COMMENT "",
  `k3` int NOT NULL COMMENT ""
) ENGINE=OLAP 
PRIMARY KEY(`k1`)
PROPERTIES ( "replication_num" = "1");
insert into t2 values(1,2,3);
select max(k1) FILTER (where FALSE) from t2;
select max(k2) FILTER (where FALSE) from t2;
select max(k3) FILTER (where FALSE) from t2;
select max(k3) FILTER (where TRUE) from t2;
select max(k3) FILTER (where k3 is not null) from t2;
select max(k3) FILTER (where k3 > 0) from t2;
select max(k3) FILTER (where k3 < 0) from t2;
select sum(k3) FILTER (where k3 > 0) from t2;
select sum(k3) FILTER (where k3 < 0) from t2;
select avg(k3) FILTER (where k3 > 0) from t2;
select avg(k3) FILTER (where k3 < 0) from t2;
select count(k3) FILTER (where k3 > 0) from t2;
select max_by(k3,k3) from t2;
select count(k3) FILTER (where k3 > 0) from t2;
select var_samp(k3) from t2;
select count(k3) FILTER (where k3 > 0) from t2;

