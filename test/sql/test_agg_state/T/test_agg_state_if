-- name: test_agg_state_if_all_functions
CREATE TABLE t1 ( 
k1  date, 
c0 boolean,
c1 tinyint(4),
c2 smallint(6),
c3 int(11),
c4 bigint(20),
c5 largeint(40),
c6 double,
c7 float,
c8 decimal(10, 2),
c9 char(100),
c10 date,
c11 datetime,
c12 array<boolean>,
c13 array<tinyint(4)>,
c14 array<smallint(6)>,
c15 array<int(11)>,
c16 array<bigint(20)>,
c17 array<largeint(40)>,
c18 array<double>,
c19 array<float>,
c20 array<DECIMAL64(10,2)>,
c21 array<char(100)>,
c22 array<date>,
c23 array<datetime>,
c24 varchar(100),
c25 json,
c26 varbinary,
c27 map<varchar(1048576),varchar(1048576)>,
c28 struct<col1 array<varchar(1048576)>>,
c29 array<varchar(100)>) DUPLICATE KEY(k1) 
DISTRIBUTED BY HASH(k1) 
PROPERTIES (  "replication_num" = "1");

INSERT INTO t1 VALUES 
(
  '2024-01-01',                    -- k1
  TRUE,                            -- c0
  127,                             -- c1
  32000,                           -- c2
  2147483647,                      -- c3
  9223372036854775807,             -- c4
  170141183460469231731687303715884105727,  -- c5 (example large integer)
  12345.6789,                      -- c6
  12345.67,                        -- c7
  123.45678901,                    -- c8
