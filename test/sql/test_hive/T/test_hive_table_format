-- name: test_hive_table_format

create external catalog hive_catalog_${uuid0} PROPERTIES (
    "type"="hive",
    "hive.metastore.uris"="${hive_metastore_uris}",
    "aws.s3.access.key"="${oss_ak}",
    "aws.s3.secret.key"="${oss_sk}",
    "aws.s3.endpoint"="${oss_endpoint}"
);

select * from hive_catalog_${uuid0}.hive_oss_db.t_regex;

drop catalog hive_catalog_${uuid0};