6	20240310.0
1	1.3
3	2.33
2	1.5
1	1.4
4	2.59
-- !result
select * from hive_test_${uuid0}.hive_oss_db.t_float_part;
-- result:
6	20240310.0
4	2.59
3	2.33
1	1.3
2	1.5
4	2.49
1	1.4
-- !result
select * from hive_test_${uuid0}.hive_oss_db.t_float_part where c1=1;
