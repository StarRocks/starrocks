PARTITION BY (dt1) 
REFRESH DEFERRED MANUAL
PROPERTIES ("partition_refresh_number"="1")
AS 
SELECT dt1, dt2, sum(int1), sum(int2) 
from mv_hive_${uuid0}.mv_hive_db_${uuid0}.partition_table t1
join non_partition_table t2 on t1.dt1 = t2.dt2 
group by dt1, dt2;
