-- name: test_audit_log
CREATE TABLE `t_audit_log` (
  `v1` int(11) NOT NULL,
  `v2` int(11) NOT NULL,
  `v3` int(11) NOT NULL
) ENGINE=OLAP
DUPLICATE KEY(`v1`)
DISTRIBUTED BY HASH(`v1`) BUCKETS 10
PROPERTIES (
 "replication_num" = "1"
);

INSERT INTO `t_audit_log` (v1, v2, v3) values
    (1, 1, 1),
    (1, 1, 2),
    (1, 1, 3);

shell: env mysql_cmd="${mysql_cmd} -D${db[0]}" url=${url} db=${db[0]} bash ${root_path}/sql/test_audit/T/test_audit_log.sh
