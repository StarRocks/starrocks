-- name: test_width_bucket
select width_bucket(5.35, 0.024, 10.06, 5);
-- result:
3
-- !result
select width_bucket(0, 0.024, 10.06, 5);
-- result:
0
-- !result
select width_bucket(11, 0.024, 10.06, 5);
-- result:
6
-- !result
select width_bucket(null, 0.024, 10.06, 5);
-- result:
None
-- !result
select width_bucket(11, null, 10.06, 5);
-- result:
None
-- !result
select width_bucket(11, 0.024, null, 5);
-- result:
None
-- !result
select width_bucket(11, 0.024, 10.06, null);
-- result:
None
-- !result
